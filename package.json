    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^20.0.0",
    "nightwatch": "^0.9.15",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "selenium-download": "^2.0.10",
    "sinon": "^2.2.0",
