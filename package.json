{
  "name": "scruffles",
  "version": "1.0.0",
  "description": "Mapping Tool",
  "main": "index.js",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --debug --output-pathinfo --watch --progress",
    "build:prod": "NODE_ENV=production webpack --optimize-minimize",
    "server:dev": "NODE_ENV=development webpack-dashboard -- webpack-dev-server",
    "server:mockapi": "json-server --watch db.json --port 3001",
    "server:express": "node server.js",
    "build:clean": "rimraf ./dist ./coverage ./logs/* ./tests/reports ./tests/screenshots",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:e2e": "nightwatch --config nightwatch.conf.js",
    "test:update-selenium": "node nightwatch.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/mrscruffles/scruffles.git"
  },
  "author": "Ryan Walega",
  "license": "ISC",
  "homepage": "https://bitbucket.org/mrscruffles/scruffles#readme",
  "dependencies": {
    "auth0-lock": "^10.15.1",
    "bootstrap": "4.0.0-alpha.6",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "json-server": "^0.10.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "scriptjs": "^2.5.8",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
